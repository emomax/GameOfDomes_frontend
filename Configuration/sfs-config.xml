<SmartFoxConfig>
	<!-- Mandatory Settings -->
  <!--<ip>85.228.182.184</ip>
  <udpIp>85.228.182.184</udpIp> -->
 <ip>localhost</ip>
  <udpIp>localhost</udpIp>
  <port>9933</port>
  <udpPort>9933</udpPort>
	<zone>BasicExamples</zone>
	<!-- End Mandatory Settings -->
	
	<debug>true</debug>
	
	<!-- For generic http port communication -->
	<httpPort>8080</httpPort>
	
	<!-- 
		BlueBox settings 
		
		useBlueBox: falls back to BlueBox if socket connection fails, default = true
		blueBoxAddress: if not passed uses the same as <ip>		
		blueBoxPort: if not passed uses the same as <httpPort>
		blueBoxPollingRate: the polling interval between requests, default 700ms
	-->
	<useBlueBox>true</useBlueBox>
	<blueBoxAddress></blueBoxAddress>
	<blueBoxPort></blueBoxPort>
	<blueBoxPollingRate></blueBoxPollingRate>
	
</SmartFoxConfig>
